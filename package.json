{
  "name": "chakannom-angular-builders",
  "version": "0.0.2",
  "description": "A consolidation of community builders for Angular build facade",
  "author": "Seon Yu",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "builders.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chakannom/angular-builders"
  },
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.19",
    "@angular-devkit/build-ng-packagr": "0.803.19",
    "karma": "4.4.1",
    "lint-staged": "^9.4.3",
    "ng-packagr": "5.7.1",
    "rimraf": "3.0.0",
    "ts-node": "8.5.2",
    "tslib": "1.10.0",
    "typescript": "3.5.3"
  },
  "builders": "builders.json",
  "lint-staged": {
    "{,src/**/}*.{md,json,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "cleanup": "rimraf dist",
    "build": "npm run cleanup && tsc && ts-node src/merge-schemes.ts",
    "release": "git push && git push --tags && npm publish"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
